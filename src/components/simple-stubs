import React from "react";
export default function Achievements({ unlocked=[] }) {
  return <div className="card"><h3 style={{marginTop:0}}>Achievements</h3>{unlocked.length===0 ? <div className="small">Inga √§nnu</div> : unlocked.map(a=> <div key={a.id}>{a.title}</div>)}</div>;
}
import React from "react";
export default function PRList({ prMap = {} }) {
  return <div className="card"><h3 style={{marginTop:0}}>PR-lista</h3>{Object.entries(prMap).length ? Object.entries(prMap).map(([k,v])=> <div key={k}>{k}: {v.best1RM}</div>) : <div className="small">Inga PR</div>}</div>;
}
